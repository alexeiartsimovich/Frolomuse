        <item name="colorAccent">@color/app_blue_secondary</item>
        <item name="colorControlActivated">@color/app_blue_secondary</item>
        <item name="colorControlHighlight">#E4E4E4</item>
        <item name="android:progressBackgroundTint">#B8B7B7</item>
    </style>

</resources>